{
    "schemaVersion": 1,

    "id": "dark-loading-screen",
    "version": "${version}",

    "name": "Dark Loading Screen",
    "description": "Makes the loading screen less retina-burning",
    "icon": "assets/dark-loading-screen/icon.png",
    "authors": ["Neecko"],
    "contact": {  
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/dark-loading-screen",
        "issues": "https://github.com/A5b84/dark-loading-screen/issues",
        "sources": "https://github.com/A5b84/dark-loading-screen"
    },
    "license": "MIT",

    "environment": "client",
    "entrypoints": {
        "client": ["io.github.a5b84.darkloadingscreen.DarkLoadingScreenMod"]
    },
    "mixins": ["dark-loading-screen.mixins.json"],

    "depends": {
        "minecraft": "<1.15"
    },

    "custom": {
        "modmenu:clientsideOnly": true
    }
}
